{
	"name": "pp2",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"build:prod": "ng build --prod -ec true -aot --output-hashing=none && npm run generate-sw && ng build --prod --aot true --app server --output-hashing=none --bundle-dependencies all && npm run tsc && npm run start",
		"build": "ng build --output-hashing=none && npm run generate-sw && ng build --app server --output-hashing=none",
		"generate-sw": "node generate-sw.js",
		"pp2-ngsw": "ngsw-config dist/public ./src/client/ngsw-config.json",
		"postpp2-ngsw": "cp node_modules/@angular/service-worker/ngsw-worker.js ./dist/public/ngsw-worker.js",
		"gg": "npm run build && npm run tsc-server && npm run generate-sw",
		"start": "node dist/server.js",
		"wp": "webpack --config dist/webpack.server.config.js --progress --colors",
		"tsc": "cd src/server && tsc"
	},
