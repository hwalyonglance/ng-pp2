{
	"name": "pp2",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"build:prod": "ng build --prod -ec true -aot --output-hashing=none && npm run generate-sw && ng build --prod --aot true --app server --output-hashing=none --bundle-dependencies all && npm run tsc && npm run start",
		"build": "ng build --output-hashing=none && npm run generate-sw && ng build --app server --output-hashing=none",
		"generate-sw": "node generate-sw.js",
		"pp2-ngsw": "ngsw-config dist/public ./src/client/ngsw-config.json",
		"postpp2-ngsw": "cp node_modules/@angular/service-worker/ngsw-worker.js ./dist/public/ngsw-worker.js",
		"gg": "npm run build && npm run tsc-server && npm run generate-sw",
		"start": "node dist/server.js",
		"wp": "webpack --config dist/webpack.server.config.js --progress --colors",
		"tsc": "cd src/server && tsc"
	},
	"dependencies": {
		"@angular/animations": "^5.1.2",
		"@angular/cdk": "^5.0.2",
		"@angular/common": "^5.1.2",
		"@angular/compiler": "^5.1.2",
		"@angular/core": "^5.1.2",
		"@angular/flex-layout": "^2.0.0-beta.12",
		"@angular/forms": "^5.1.2",
		"@angular/material": "^5.0.2",
		"@angular/platform-browser": "^5.1.2",
		"@angular/platform-browser-dynamic": "^5.1.2",
		"@angular/platform-server": "^5.1.2",
		"@angular/platform-webworker": "^5.1.2",
		"@angular/platform-webworker-dynamic": "^5.1.2",
		"@angular/router": "^5.1.2",
		"@angular/service-worker": "^5.1.2",
		"@nguniversal/common": "^5.0.0-beta.5",
		"@nguniversal/express-engine": "^5.0.0-beta.5",
		"@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
		"@angular/fire": "latest",
		"classlist.js": "latest",
		"core-js": "latest",
		"cors": "^2.8.4",
		"firebase": "^4.8.0",
		"hammerjs": "latest",
		"intl": "latest",
		"reflect-metadata": "*",
		"rxjs": "latest",
		"web-animations-js": "latest",
		"zone.js": "latest"
	},
	"devDependencies": {
		"@angular/cli": "^1.6.3",
		"@angular/compiler-cli": "^5.1.2",
		"@angular/language-service": "^5.1.2",
		"typescript": "^2.5.3",
		"webpack": "^3.10.0"
	}
}
